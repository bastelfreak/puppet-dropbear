{
  "name": "sbadia-dropbear",
  "version": "1.0.3",
  "author": "Sebastien Badia, Kyle Anderson",
  "summary": "Puppet dropbear module",
  "license": "GPL-3.0",
  "source": "git://github.com/sbadia/puppet-dropbear.git",
  "project_page": "https://github.com/sbadia/puppet-dropbear/",
  "issues_url": "https://github.com/sbadia/puppet-dropbear/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["7", "8", "9"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["14.04", "16.04"]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": ["24", "25"]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["6", "7"]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 5.0.0"
    }
  ],
  "description": "Module to install/manage Dropbear SSH server using puppet",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.0.0 <5.0.0"
    }
  ]
}
